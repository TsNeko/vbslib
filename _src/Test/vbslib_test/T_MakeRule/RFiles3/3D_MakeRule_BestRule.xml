<?xml version="1.0" encoding="UTF-8"?>
<MakeRuleOfRevisionFolder>

	<Variable  name="${Files}"  value="."  type="FullPathType"/>

	<MakeRule>
		<Output path="${Files}\Step1\*"/>
		<Input_ path="${Files}\Step2A\*"/>
		<Input_ path="${Files}\Step2B\*"/>
		<Command></Command>
		<RevisionSet>01, 02, 22</RevisionSet>
	</MakeRule>

	<MakeRule>
		<Output path="${Files}\NotFoundA\*"/>
		<Output path="${Files}\Step2B\*"/>
		<Input_ path="${Files}\Step3A\*"/>
		<Command></Command>
		<RevisionSet>00, 22, 03</RevisionSet>
	</MakeRule>

	<MakeRule>
		<Output path="${Files}\Step2A\*"/>
		<Output path="${Files}\Step2B\*"/>
		<Input_ path="${Files}\Step3B\*"/>
		<Command></Command>
		<RevisionSet>02, 22, 03</RevisionSet>
	</MakeRule>

	<MakeRule>
		<Output path="${Files}\Step2A\*"/>
		<Output path="${Files}\NotFoundB\*"/>
		<Input_ path="${Files}\Step3C\*"/>
		<Command></Command>
		<RevisionSet>02, 00, 03</RevisionSet>
	</MakeRule>

</MakeRuleOfRevisionFolder>
