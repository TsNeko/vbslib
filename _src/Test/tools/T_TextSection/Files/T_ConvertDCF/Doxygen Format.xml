<?xml version="1.0" encoding="Shift-JIS"?>
<DocumetCommentFormat>

<Text id="File"><![CDATA[
/**
* @file  ${Name}
* @brief  ${Brief}${Descriptions}
*/
]]></Text>


<Text id="Function"><![CDATA[
/**
* @brief   ${Brief}
*
* @param   ${Name} ${Brief}
* @return  ${ReturnValue}${Descriptions}
*/
]]></Text>


<Text id="Structure"><![CDATA[
/**
* @struct  ${Name}
* @brief  ${Brief}${Descriptions}
*/
]]></Text>


<Text id="Enumeration"><![CDATA[
/**
* @enum   ${Name}
* @brief  ${Brief}${Descriptions}
*/
]]></Text>


<Text id="Type"><![CDATA[
/**
* @typedef  ${Name}
* @brief  ${Brief}${Descriptions}
*/
]]></Text>


<Text id="Macro"><![CDATA[
/**
* @def  ${Name}
* @brief  ${Brief}
* @param   ${Name} ${Brief}
* @return  ${ReturnValue}${Descriptions}
*/
]]></Text>


<Text id="Constant"><![CDATA[
/**
* @def  ${Name}
* @brief  ${Brief}${Descriptions}
*/
]]></Text>


<Text id="Constants"><![CDATA[
/**
* @def  ${Name}
* @brief  ${Brief}${Descriptions}
*/
]]></Text>


<Text id="Variable"><![CDATA[
/** ${Brief} */
]]></Text>


<Text id="NoParameter"><![CDATA[
* @par Parameters
*    None
]]></Text>


<Text id="SubTitle">@par ${SubTitle}</Text>
<Text id="Link">@ref ${Keyword}</Text>
<Text id="CodeStart">@code</Text>
<Text id="CodeOver">@endcode</Text>
<Text id="DefinitionList">- ${Name} - ${Brief}</Text>


</DocumetCommentFormat>
